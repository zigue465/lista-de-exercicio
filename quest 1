#include <stdio.h>

int main() {
    int matriz[4][4];
    int i, j;
    int contador = 0;

    for (i = 0; i < 4; i++) {
        for (j = 0; j < 4; j++) {
            printf("Digite um valor: ");
            scanf("%d", &matriz[i][j]);
        }
    }

    for (i = 0; i < 4; i++) {
        for (j = 0; j < 4; j++) {
            if (matriz[i][j] > 10) {
                contador++;
            }
        }
    }
  
  printf("A matriz possui %d valores maiores que 10.\n", contador);

    return 0;
}
